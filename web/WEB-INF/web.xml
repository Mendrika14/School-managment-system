    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S_informatique</servlet-name>
        <url-pattern>/S_informatique</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S_design</servlet-name>
        <url-pattern>/S_design</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
