        <servlet-name>S_inscription</servlet-name>
        <url-pattern>/S_inscription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S_ecole</servlet-name>
        <url-pattern>/S_ecole</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>S_admi</servlet-name>
        <url-pattern>/S_admi</url-pattern>
