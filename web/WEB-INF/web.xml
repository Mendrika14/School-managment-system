    </servlet>
    <servlet>
        <servlet-name>S_admi</servlet-name>
        <servlet-class>controlleur.S_admi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>S_ecole_region</servlet-name>
        <servlet-class>controlleur.S_ecole_region</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>S_informatique</servlet-name>
